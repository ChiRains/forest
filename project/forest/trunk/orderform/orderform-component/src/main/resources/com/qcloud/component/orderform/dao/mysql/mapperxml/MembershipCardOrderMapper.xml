<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.qcloud.component.orderform.dao.mysql.mapper.MembershipCardOrderMapper">          
    <select id="list4query" parameterType="java.util.HashMap" resultType="MembershipCardOrder">  
        select * from `orderform_membership_card_order` limit #{start},#{count}
